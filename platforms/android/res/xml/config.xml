<?xml version='1.0' encoding='utf-8'?>
<widget id="ph.com.fsbergado.randomplate" version="2.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SQLitePlugin">
        <param name="android-package" value="org.pgsqlite.SQLitePlugin" />
    </feature>
    <feature name="AppVersion">
        <param name="android-package" value="uk.co.whiteoctober.cordova.AppVersion" />
    </feature>
    <name>Random Plate Number Generator</name>
    <description>
        A mobile app for generating random plate numbers
    </description>
    <icon src="images/logo.png" />
    <author email="ferdiebergado@yahoo.com" href="http://cordova.io">
        ferdiebergado@yahoo.com
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
